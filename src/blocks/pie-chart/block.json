{
	"apiVersion": 2,
	"name": "charts-blocks/pie-chart",
	"category": "charts_blocks",
	"textdomain": "charts-blocks",
	"attributes": {
		"title": {
			"type": "string",
			"default": "Bar Graph of Colours"
		},
		"showTitle": {
			"type": "boolean",
			"default": true
		},
		"labels": {
			"type": "array",
			"default": ["Red", "Blue", "Yellow"]
		},
		"chartdata": {
			"type": "array",
			"default": [300, 50, 100]
		}
	},
	"supports": {
		"align": ["wide", "full"],
		"html": false
	}
}
